@model System.Collections.Generic.IEnumerable<Register.DomainModel.User>

@{
    ViewBag.Title = "Show users";      
}


<div class="container">
    <div class="row">
        <div class="col-md-10">
            <h2>List of Users</h2>
            <table>
                <tbody>
                    @foreach (var user in this.Model)
                    {
                        <tr>
                            <td>@user.UserID</td>
                            <td>@user.UserName</td>
                            <td>@user.FirstName</td>
                            <td>@user.LastName</td>
                            <td>@user.CityAddress</td>
                            <td>@user.ContactNumber</td>
                        </tr>
                    }
                </tbody>
            </table>

        </div>
    </div>

</div>